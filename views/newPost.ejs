<%- include("partials/header"); -%>

<h1>Create Post</h1>
<form class="" action="/posts/new" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle" required />
    <label>Post</label>
    <textarea class="form-control" name="postContent" rows="5" cols="30" required></textarea>
  </div>
  <button class="btn btn-secondary m-2" onclick="history.back(-1)">Back</button>
  <button class="btn btn-primary m-2" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>
